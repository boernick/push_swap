{
    "configurations": [
        {
            "name": "C/C++: gcc build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": ["150", "2", "-100", "50", "40000", "-40", "300", "9999", "-300", "75", "0", "1", "-1", "200", "500", "100", "-500", "250", "-200", "60", "-150", "1000", "3", "45", "-45", "400", "-400", "5000", "-5000", "30", "-30", "20", "-20", "10", "-10", "5", "-5", "15", "-15", "25", "-25", "35", "-35", "40", "-40", "7", "-7", "8", "-8", "9", "-9"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: gcc build active file",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ],
    "version": "2.0.0"
}
